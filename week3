#week 3[a]
  import java.util.*;
class bsj3a{
void bs(int a,int b)
{
System.out.println("add : "+(a+b));
}
public static void main(String[] args)
{
bsj3a f4 = new bsj3a();
f4.bs(1,2);
}
}
